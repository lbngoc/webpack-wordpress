{
  "name": "palmerhq/presspack",
  "description": "Minimalist wordpress theme",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org"
  }],
  "require": {
    "wpackagist-plugin/advanced-custom-fields": "4.4.11",
    "wpackagist-plugin/members": "1.1.3",
    "wpackagist-plugin/acf-to-rest-api": "2.2.1",
    "roots/soil": "3.7.1"
  },
  "extra": {
    "installer-paths": {
      "www/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "www/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "www/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
